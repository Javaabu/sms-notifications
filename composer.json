{
    "name": "javaabu/sms-notifications",
    "description": "Adds SMS notifications to Laravel projects with support for multiple providers.",
    "homepage": "https://github.com/Javaabu/sms-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Arushad Ahmed",
            "email": "arushad@javaabu.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Javaabu\\SmsNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Javaabu\\SmsNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Javaabu\\SmsNotifications\\SmsNotificationsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
